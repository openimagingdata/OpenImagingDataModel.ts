{
  "name": "@openimagingdata/openimagingdatamodel",
  "version": "0.0.0",
  "types": "./dist/index.d.ts",
  "description": "Tooling for managing data context of a medical imaging exam.",
  "repository": {
    "type": "git",
    "url": "https://github.com/openimagingdata/OpenImagingDataModel.ts"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "openimagingdata",
      "email": "tarik@alkasab.org"
    },
    {
      "name": "Kyle Macri",
      "email": ""
    }
  ],
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "dev": "vitest",
    "tsc": "tsc",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "@effect-ts/core": "^0.58.0",
    "@effect/schema": "^0.69.2",
    "dotenv": "^16.4.5",
    "effect": "^3.6.1",
    "fast-check": "^2.0.0",
    "openai": "^4.58.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/eslint": "^8.56.10",
    "console-fail-test": "^0.2.3",
    "eslint": "^9.4.0",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^7.11.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "provenance": true
  }
}
